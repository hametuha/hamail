{
  "env": {
    "browser": true,
  },
  "globals": {
    "jQuery": false,
    "Backbone": false,
    "_": false,
    "HamailRecipients": false
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "plugins": [
  ],
  "rules": {
    "indent": [
      "error",
      2
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ]
  }
}