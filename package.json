{
  "name": "hamail",
  "version": "1.0.0",
  "description": "A WordPress plugin to enable balk email.",
  "main": "gulpfile.js",
  "scripts": {
    "start": "wp-env start",
    "update": "wp-env start --update",
    "package": "gulp build",
    "watch": "gulp watch",
    "gulp": "gulp",
    "jed": "po2json languages/hamail-$LOCALE.po languages/hamail-$LOCALE-hamail-i18n.json -f jed",
    "jed-ja": "po2json languages/hamail-ja.po languages/hamail-ja-hamail-i18n.json -f jed",
    "env": "wp-env",
    "cli": "wp-env run cli wp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hametuha/hamail.git"
  },
  "keywords": [
    "WordPress",
    "SendGrid"
  ],
  "author": "Hametuha INC.",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/hametuha/hamail/issues"
  },
  "homepage": "https://github.com/hametuha/hamail#readme",
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/plugin-transform-react-jsx": "^7.0.0",
    "@babel/preset-env": "^7.5.4",
    "@wordpress/env": "^2.1.0",
    "@wordpress/eslint-plugin": "^7.3.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "eslint": "^7.12.1",
    "gulp": "^4.0.0",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-eslint": "^6.0.0",
    "gulp-imagemin": "^7.1.0",
    "gulp-load-plugins": "^2.0.5",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.0",
    "gulp-sass-glob": "^1.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-stylelint": "^13.0.0",
    "gulp-svgmin": "^3.0.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngquant": "^9.0.1",
    "license-webpack-plugin": "^2.1.3",
    "merge-stream": "^2.0.0",
    "po2json": "^1.0.0-alpha",
    "stylelint": "^13.7.2",
    "stylelint-config-wordpress": "^17.0.0",
    "vinyl-named": "^1.1.0",
    "webpack": "^5.3.0",
    "webpack-stream": "^6.1.0"
  }
}
